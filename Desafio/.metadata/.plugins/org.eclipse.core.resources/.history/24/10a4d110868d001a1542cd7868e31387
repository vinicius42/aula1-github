package prova;

import java.util.Scanner;

public class Debito extends FormaDePagamento {
	public String banco = "";
	public String agencia = "";
	public String conta = "";

	public Debito(String b, String a, double v, String c) {
		banco = b;
		agencia = a;
		valor = v;
		conta = c;
	}
	
	public Debito() {
		banco = "N/A";
		conta = "";
		valor = 0.0;
	}

	public boolean Pagar(Debito sacada, Debito depositada, Maquina maq) {
		return true;

	}

}
